{
    "name": "Extension Pop Up",
    "version": "1.0",
    "description": "Employee Info!",  
    "manifest_version": 2,
    "icons": {
      "128": "icon.png"
    },
    "permissions": [
      "activeTab",
      "contextMenus",
      "storage",
      "http://localhost/*"
    ],
    "browser_action": {  
      "default_icon": "icon.png",  
      "default_popup": "index.html"  
    }
}